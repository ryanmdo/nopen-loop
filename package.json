    "concurrently": "^3.5.1",
    "express": "^4.16.2",
    "mongoose": "^5.1.2",
    "nodemon": "^1.17.5",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-router-dom": "^4.2.2",
    "react-scripts": "^1.1.5",
    "superagent": "^3.8.3"
  }
}
